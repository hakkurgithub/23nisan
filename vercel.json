{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/oyunlar",
      "dest": "/oyunlar.html"
    },
    {
      "src": "/atolyeler",
      "dest": "/atolyeler.html"
    },
    {
      "src": "/etkinlikler",
      "dest": "/etkinlikler.html"
    },
    {
      "src": "/sanat",
      "dest": "/sanat.html"
    },
    {
      "src": "/blog",
      "dest": "/blog.html"
    },
    {
      "src": "/giris",
      "dest": "/giris.html"
    },
    {
      "src": "/kayit",
      "dest": "/kayit.html"
    },
    {
      "src": "/parmak-boyama-projesi",
      "dest": "/parmak-boyama-projesi.html"
    },
    {
      "src": "/parmak-boyama-video",
      "dest": "/parmak-boyama-video.html"
    },
    {
      "src": "/parmak-boyama-video-real",
      "dest": "/parmak-boyama-video-real.html"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|mp4|webm|woff|woff2|ttf|eot))",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    }
  ]
}